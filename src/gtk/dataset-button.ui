<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <template class="DatasetButton">
        <object class="GtkButton" id="button">
            <signal name="clicked" handler="dataset_button_click_handler" />
            <child>
                <object class="GtkBox">
                    <property name="orientation">vertical</property>
                    <property name="spacing">10</property>
                    <property name="valign">center</property>
                    <child>
                        <object class="GtkImage" id="image">
                            <property name="icon-name">image-x-generic</property>
                            <property name="resource">
                                /com/github/psycomentis/DataMiningGtk/assets/images/dataset1.png</property>
                            <property name="pixel-size">150</property>
                            <property name="use-fallback">true</property>
                        </object>
                    </child>
                    <child>
                        <object class="GtkLabel" id="label">
                            <property name="label">Dataset Label</property>
                        </object>
                    </child>
                </object>
            </child>
        </object>
    </template>
</interface>