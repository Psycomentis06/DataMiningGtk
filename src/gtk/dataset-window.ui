    <template class="DatasetWindow" parent="AdwApplicationWindow">
        <property name="default-width">800</property>
        <property name="default-height">400</property>
        <property name="title">Dataset Window</property>
        <child>
            <object class="GtkBox">
                <property name="orientation">vertical</property>
                <child>
                    <object class="GtkHeaderBar">
                        <child type="end">
                            <object class="GtkMenuButton">
                                <property name="icon-name">view-more-symbolic</property>
                                <property name="menu-model">primary_menu</property>
                            </object>
                        </child>
                        <child type="start">
                            <object class="GtkButton" id="sidebar_toggle_btn">
                                <property name="icon-name">pan-start-symbolic</property>
                                <!-- <property name="action-name">app.dataset_window-sidebar-toggle</property>-->
                                <signal name="clicked" handler="dataset_sidebar_toggle"></signal>
                            </object>
                        </child>
                    </object>
                </child>
                <child>
                    <!-- Ui content-->
                    <object class="GtkBox" id="container">
                        <property name="orientation">horizontal</property>
                        <property name="homogeneous">false</property>
                        <property name="spacing">20</property>
                        <child>
                          <!-- Sidebar -->
                            <object class="GtkRevealer" id='revealer'>
                                <property name="transition-duration">400</property>
                                <property name="transition-type">slide-right</property>
                                <property name="vexpand">true</property>
                                <child>
                                    <object class="GtkBox">
                                        <property name="orientation">vertical</property>
                                        <property name="homogeneous">false</property>
                                        <property name="spacing">20.0</property>
                                        <property name="margin-bottom">30.0</property>
                                        <child>
                                          <object class="GtkBox">
                                            <property name="orientation">vertical</property>
                                            <child>
                                              <object class="GtkLabel">
                                                <property name="label" > Support Value (%) </property>
                                                <property name="halign" >start</property>
                                              </object>
                                            </child>
                                            <child>
                                              <object class="GtkSpinButton" id="support_spin_button">
                                                <property name="numeric">true</property>
                                                <property name="halign">start</property>
                                                <property name="width-chars">9</property>
                                                <!-- <property name="palceholder-text">Support Value</property> -->
                                              </object>
                                            </child>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkBox">
                                            <property name="orientation">vertical</property>
                                            <child>
                                              <object class="GtkLabel">
                                                <property name="label" > Dataset size (%) </property>
                                                <property name="halign" >start</property>
                                              </object>
                                            </child>
                                            <child>
                                              <object class="GtkSpinButton">
                                                <property name="numeric">true</property>
                                                <property name="halign">start</property>
                                                <property name="width-chars">9</property>
                                                <!-- <property name="palceholder-text">Support Value</property> -->
                                              </object>
                                            </child>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkBox">
                                            <property name="orientation">vertical</property>
                                            <child>
                                              <object class="GtkLabel">
                                                <property name="label" > Dataset Shuffle Value (%) </property>
                                                <property name="halign" >start</property>
                                              </object>
                                            </child>
                                            <child>
                                              <object class="GtkSpinButton">
                                                <property name="numeric">true</property>
                                                <property name="halign">start</property>
                                                <property name="width-chars">9</property>
                                                <!-- <property name="palceholder-text">Support Value</property> -->
                                              </object>
                                            </child>
                                          </object>
                                        </child>
                                    </object>
                                </child>
                            </object>
                        </child>
                        <child>
                          <!-- Content -->
                            <object class="GtkBox" id="tree_view_container">
                                <property name="hexpand">true</property>
                                <property name="orientation">vertical</property>
                                <property name="spacing">20.0</property>
                                <child>
                                    <object class="GtkLabel">
                                        <property name="label">Content</property>
                                    </object>
                                </child>
                                <!-- <child> -->
                                <!--   <object class="GtkTreeView" id="dataset_tree_view"> -->
                                <!--     <property name="model"> list_store_model </property> -->
                                <!--     <child> -->
                                <!--       <object class="GtkTreeViewColumn" id="dataset_tree_view_column1"> -->
                                <!--         <property name="title">Column 1</property> -->
                                <!--         <child> -->
                                <!--           <object class="GtkCellRendererText" id="cell_renderer1"> -->
                                <!--             <property name="text"> Testing </property> -->
                                <!--           </object> -->
                                <!--         </child> -->
                                <!--       </object> -->
                                <!--     </child> -->
                                <!--     <child> -->
                                <!--       <object class="GtkTreeViewColumn" id="dataset_tree_view_column2"> -->
                                <!--         <property name="title">Column 2</property> -->
                                <!--         <child> -->
                                <!--           <object class="GtkCellRendererText" id="cell_renderer2"> -->
                                <!--             <property name="text"> Testing </property> -->
                                <!--           </object> -->
                                <!--         </child> -->
                                <!--       </object> -->
                                <!--     </child> -->
                                <!--   </object> -->
                                <!-- </child> -->
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>
    </template>
    <menu id="primary_menu">
        <section>
            <item>
                <attribute name="label" translatable="yes">_Preferences</attribute>
                <attribute name="action">app.preferences</attribute>
            </item>
            <item>
                <attribute name="label" translatable="yes">_Keyboard Shortcuts</attribute>
                <attribute name="action">win.show-help-overlay</attribute>
            </item>
            <item>
                <attribute name="label" translatable="yes">_About DataMiningGtk</attribute>
                <attribute name="action">app.about</attribute>
            </item>
        </section>
    </menu>
</interface>
